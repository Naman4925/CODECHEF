#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	for(int i=0;i<n;i++)
	{
	    int a,x,p,b,c,d;
	    cin>>a>>x>>p;
	    b=x*3;
	    c=a-x;
	    d=b-c;
	    if(d>=p)
	    {
	        cout<<"Pass"<<endl;
	    }
	    else
	    {
	        cout<<"Fail"<<endl;
	    }
	}
	return 0;
}
