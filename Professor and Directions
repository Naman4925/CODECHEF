#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s;
        cin>>s;
        string str = "NO";
        for(int i=0;i<n-1;i++)
        {
            if((s[i]=='R' && s[i+1]=='R') || (s[i] =='L' && s[i+1] =='L'))
            {
                str = "YES";
                break;
            }
        }
        cout<<str<<endl;
    }
	// your code goes here
	return 0;
}
