#include "stdio.h"
#define loop(a) for(int i=0;i<a;i++)
int main()
{
    int n,a,b,c;
    scanf("%d",&n);
    loop(n){
        scanf("%d %d %d",&a,&b,&c);
        if((a==7) || (b==7) || (c==7)){
            printf("YES\n");
        }
        else{
            printf("NO\n");
        }
    }
    

	// your code goes here
	
	return 0;
}
